task.spawn(function()
	while wait() do
		pcall(function()
			getgenv().rconsoleprint = "DEVIL Script"
		end)
		pcall(function()
			getgenv().print = "DEVIL Script"
		end)
		pcall(function()
			getgenv().warn = "DEVIL Script"
		end)
		pcall(function()
			getgenv().error = "DEVIL Script"
		end)
		pcall(function()
			getgenv().consoleprint = "DEVIL Script"
		end)
	end
end)
